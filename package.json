{
  "name": "scaffold-frontend",
  "version": "0.10.5",
  "main": "index.js",
  "repository": "git@github.com:framelunch/scaffold-frontend.git",
  "author": "Yuichi Ikeda <ikeda@framelunch.jp>",
  "license": "MIT",
  "engines": {
    "node": ">=8.11.1"
  },
  "browserslist": [
    "ie 11",
    "ios >= 10.3",
    "> 1% in JP",
    "not ie <= 10",
    "not chrome 49"
  ],
  "lint-staged": {
    "subTaskConcurrency": 1,
    "linters": {
      "*.{js,jsx,ts,tsx}": [
        "eslint --fix",
        "git add"
      ],
      "*.css": [
        "stylelint --fix",
        "git add"
      ],
      "*.json": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "scripts": {
    "start": "run-p start:*",
    "start:gulp": "cross-env NODE_ENV=development gulp",
    "start:webpack": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --history-api-fallback --progress --color --config tools/webpack/development.js",
    "prebuild": "run-s typecheck",
    "build": "run-s build:gulp build:webpack",
    "build:gulp": "cross-env NODE_ENV=production gulp build",
    "build:webpack": "cross-env NODE_ENV=production webpack --progress --config tools/webpack/production.js",
    "build:check": "run-s build server",
    "build:diff": "node tools/getBuildDiff",
    "server": "http-server ./build -p 23000 -o --cors",
    "spreadsheet": "node tools/spreadsheetToJson",
    "typecheck": "tsc --noEmit -p tsconfig.production.json",
    "safe-guard": "lint-staged",
    "lint": "run-p lint:*",
    "lint:style": "stylelint 'src/**/*.css'",
    "lint:script": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "fix": "run-p fix:*",
    "fix:style": "stylelint --fix 'src/**/*.css'",
    "fix:script": "eslint --ext .js,.jsx,.ts,.tsx --fix src/",
    "fix:json": "prettier --write 'src/**/*.json'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s safe-guard"
    }
  },
  "private": true,
  "dependencies": {
    "date-fns": "^1.30.1",
    "jquery": "^3.4.1",
    "jquery-touch-events": "^1.0.7",
    "libraries-frontend-framelunch": "framelunch/libraries-frontend-framelunch#0.5.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "autoprefixer": "^9.6.0",
    "browser-sync": "^2.26.5",
    "cache-loader": "^3.0.1",
    "child_process": "^1.0.2",
    "cross-env": "^5.2.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.0.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.1.0",
    "fork-ts-checker-webpack-plugin": "^1.3.5",
    "fs-extra": "^8.0.1",
    "google-spreadsheet": "^2.0.7",
    "gulp": "^4.0.2",
    "gulp-ejs": "^4.0.0",
    "gulp-ext-replace": "^0.3.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^6.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-rev": "^9.0.0",
    "gulp-rev-replace": "^0.4.3",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-wait": "^0.0.2",
    "http-server": "^0.11.1",
    "husky": "^2.3.0",
    "imagemin-gif2webp": "^2.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-webp": "^5.1.0",
    "lint-staged": "^8.1.7",
    "node-sass-package-importer": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "postcss-calc": "^7.0.1",
    "postcss-color-hex-alpha": "^5.0.3",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^8.0.10",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.1.2",
    "postcss-url": "^8.0.0",
    "prettier": "^1.17.1",
    "raw-loader": "^2.0.0",
    "require-dir": "^1.2.0",
    "rimraf": "^2.6.3",
    "stylelint": "^10.0.1",
    "stylelint-config-framelunch": "framelunch/stylelint-config-framelunch#v0.4.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recess-order": "^2.0.2",
    "svg-inline-loader": "^0.8.0",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.1"
  }
}
