{
  "name": "scaffold-frontend",
  "version": "0.7.2",
  "main": "index.js",
  "repository": "git@github.com:framelunch/scaffold-frontend.git",
  "author": "Yuichi Ikeda <ikeda@framelunch.jp>",
  "license": "MIT",
  "engines": {
    "node": ">=8.11.1"
  },
  "browserslist": [
    "ie 11",
    "ios >= 10",
    "> 1% in JP",
    "not ie <= 10",
    "not chrome 49"
  ],
  "lint-staged": {
    "subTaskConcurrency": 1,
    "linters": {
      "*.{js,jsx}": [
        "eslint --fix",
        "git add"
      ],
      "*.css": [
        "prettier --write",
        "stylelint",
        "git add"
      ],
      "*.json": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development gulp",
    "prebuild": "run-s typecheck",
    "build": "cross-env NODE_ENV=production gulp build",
    "build:check": "run-s build server",
    "build:diff": "node tools/getBuildDiff",
    "server": "http-server ./build -p 23000 -o --cors",
    "spreadsheet": "node tools/spreadsheetToJson",
    "typecheck": "flow",
    "typeinstall": "flow-typed install",
    "safe-guard": "lint-staged",
    "lint": "run-p lint:*",
    "lint:style": "stylelint 'src/**/*.css'",
    "lint:script": "eslint --ext .js,.jsx src/",
    "fix": "run-p prettier:*",
    "fix:style": "prettier --write 'src/**/*.css'",
    "fix:script": "eslint --ext .js,.jsx --fix 'src/**/*.{js,jsx}'",
    "fix:json": "prettier --write 'src/**/*.json'",
    "precommit": "run-s typecheck safe-guard"
  },
  "private": true,
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "date-fns": "^1.29.0",
    "jquery": "^3.2.1",
    "jquery-touch-events": "^1.0.7",
    "libraries-frontend-framelunch": "framelunch/libraries-frontend-framelunch#0.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-date-fns": "^0.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "browser-sync": "^2.24.5",
    "cache-loader": "^1.1.0",
    "child_process": "^1.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.2",
    "dotenv": "^6.0.0",
    "eslint": "^4.3.0",
    "eslint-config-framelunch": "framelunch/eslint-config-framelunch#v0.4.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype-errors": "^3.6.0",
    "eslint-plugin-prettier": "^2.6.1",
    "flow-bin": "^0.76.0",
    "flow-typed": "^2.1.5",
    "fs-extra": "^7.0.0",
    "google-spreadsheet": "^2.0.5",
    "gulp": "^3.9.1",
    "gulp-ejs": "^3.1.3",
    "gulp-htmlmin": "^4.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-rev": "^8.0.0",
    "gulp-rev-replace": "^0.4.3",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-wait": "^0.0.2",
    "http-server": "^0.11.1",
    "husky": "^0.14.3",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-pngquant": "^5.0.1",
    "lint-staged": "^7.2.0",
    "npm-run-all": "^4.1.3",
    "postcss-color-hex-alpha": "^3.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^7.0.0",
    "postcss-fixes": "^2.0.1",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.1.5",
    "postcss-nested": "^3.0.0",
    "postcss-url": "^7.1.2",
    "prettier": "^1.13.7",
    "raw-loader": "^0.5.1",
    "require-dir": "^1.0.0",
    "rimraf": "^2.6.2",
    "run-sequence": "^2.1.0",
    "style-loader": "^0.21.0",
    "stylelint-config-framelunch": "framelunch/stylelint-config-framelunch#v0.4.0",
    "svg-inline-loader": "^0.8.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "webpack-stream": "^5.0.0"
  }
}
